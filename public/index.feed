td style=\"text-align: \"\u003e\u003ccode\u003evenezuela\u003c/code\u003e\u003c/td\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1fb;\u0026#x1f1ec;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003ebritish_virgin_islands\u003c/code\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1fb;\u0026#x1f1ee;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003eus_virgin_islands\u003c/code\u003e\u003c/td\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1fb;\u0026#x1f1f3;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003evietnam\u003c/code\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1fb;\u0026#x1f1fa;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003evanuatu\u003c/code\u003e\u003c/td\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1fc;\u0026#x1f1eb;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003ewallis_futuna\u003c/code\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1fc;\u0026#x1f1f8;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003esamoa\u003c/code\u003e\u003c/td\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1fd;\u0026#x1f1f0;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003ekosovo\u003c/code\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1fe;\u0026#x1f1ea;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003eyemen\u003c/code\u003e\u003c/td\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1fe;\u0026#x1f1f9;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003emayotte\u003c/code\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1ff;\u0026#x1f1e6;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003esouth_africa\u003c/code\u003e\u003c/td\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1ff;\u0026#x1f1f2;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003ezambia\u003c/code\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u0026#x1f1ff;\u0026#x1f1fc;\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003ccode\u003ezimbabwe\u003c/code\u003e\u003c/td\u003e\r\n            \u003ctd style=\"text-align: center\"\u003e\u003c/td\u003e\r\n            \u003ctd style=\"text-align: \"\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n  \u003c/table\u003e\r\n\u003c/div\u003e",
